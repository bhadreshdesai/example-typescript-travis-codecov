{
  "name": "example-typescript-travis-codecov",
  "version": "1.0.0",
  "description": "An example typescript project using travis and codecov",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon src/index.ts",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.10",
    "jest": "26.6.3",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  },
  "devDependencies": {
    "nodemon": "1.18.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": ["/node_modules/"],
    "verbose": true
  },
  "keywords": []
}
